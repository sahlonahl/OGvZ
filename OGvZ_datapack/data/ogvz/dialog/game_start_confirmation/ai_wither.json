{
  "type": "minecraft:multi_action",
  "title": {
    "text": "Game Start Confirmation",
    "color": "white",
    "bold": true
  },
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": {
        "text": "Setup is complete!",
        "color": "green"
      }
    },
    {
      "type": "minecraft:plain_message",
